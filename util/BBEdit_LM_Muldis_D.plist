<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>BBEditDocumentType</key>
  <string>CodelessLanguageModule</string>
  <key>BBLMLanguageDisplayName</key>
  <string>Muldis D Plain Text</string>
  <key>BBLMLanguageCode</key>
  <string>MDPT</string>
  <key>BBLMSuffixMap</key>
  <array>
    <dict>
      <key>BBLMLanguageSuffix</key>
      <string>.mdpt</string>
    </dict>
  </array>
  <key>BBLMColorsSyntax</key>
  <true/>
  <key>BBLMScansFunctions</key>
  <true/>
  <key>BBLMIsCaseSensitive</key>
  <true/>
  <key>BBLMKeywordList</key>
  <array>
    <string>Muldis_D</string>
    <string>Plain_Text</string>
    <string>abstract</string>
    <string>and_provides_its_default</string>
    <string>associative</string>
    <string>commutative</string>
    <string>composes</string>
    <string>constant</string>
    <string>default</string>
    <string>else</string>
    <string>fail</string>
    <string>folded</string>
    <string>from</string>
    <string>function</string>
    <string>given</string>
    <string>has_segment</string>
    <string>idempotent</string>
    <string>identity</string>
    <string>if</string>
    <string>implements</string>
    <string>is</string>
    <string>normalize</string>
    <string>not</string>
    <string>of</string>
    <string>overrides</string>
    <string>package</string>
    <string>package_segment</string>
    <string>procedure</string>
    <string>requires</string>
    <string>requires_implements</string>
    <string>script</string>
    <string>searching</string>
    <string>singleton</string>
    <string>synonym</string>
    <string>then</string>
    <string>to</string>
    <string>type</string>
    <string>union</string>
    <string>using</string>
    <string>via</string>
    <string>virtual</string>
    <string>when</string>
    <string>where</string>
  </array>
  <key>Language Features</key>
  <dict>
    <key>Identifier and Keyword Character Class</key>
    <string><![CDATA[a-zA-Z0-9_]]></string>

    <key>Comment Pattern</key>
    <string><![CDATA[(?xs:
      (` [^`]* `)
    )]]></string>

    <key>String Pattern</key>
    <string><![CDATA[(?xs:
      (
        (false | true)
      | (' [^']* ')
      | ((?<!\w) [0-9] [0-9A-Za-z_]* (\. [0-9A-Za-z_]+)?)
      )
    )]]></string>

    <key>Function Pattern</key>
    <string><![CDATA[(?xs:
      (?P<function_name>
        (
            [a-zA-Z_][a-zA-Z0-9_]*
          | " [^"]* "
        )
        (
          \s* [.:] \s*
          (
              [a-zA-Z_][a-zA-Z0-9_]*
            | " [^"]* "
          )
        )*
      )
      \s* ::= \s*
      (type|constant|function|procedure|synonym)
    )]]></string>
  </dict>
</dict>
</plist>
